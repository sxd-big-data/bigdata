<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bigdata.mysql.mapper.OrderDeliverDataMapper">
    <select id="selectAll" resultType="OrderDeliverDetailData">
        select * from order_deliver_detail_data 
        where 1=1 and deliver_date >= '2020-12-01'
        <if test="ew!=null">
          ${ew.customSqlSegment}
        </if>
    </select>
</mapper>